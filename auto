            firesignal(game.Players.LocalPlayer.PlayerGui.Start.Skip.MouseButton1Click) 
            task.wait()
    end
    end
    end)
    
    spawn(function()
    while true do
        wait(1)
    if game.Players.LocalPlayer.PlayerGui:FindFirstChild('Start') then
            firesignal(game.Players.LocalPlayer.PlayerGui.Start.Play.MouseButton1Click) 
            task.wait()
    end
    end
    end)

