spawn(function()
    wait(30)
while true and task.wait(2) do
if game.PlaceId == 5151400895 then
break
end
if game:GetService("Workspace").Living[game.Players.LocalPlayer.Name].Stats.Defense.Value >= 300000000 and game:GetService("ReplicatedStorage").Datas[game.Players.LocalPlayer.UserId].Zeni.Value >= 30000 then
    local A_1 = "Vills Planet"
    local Event = game:GetService("ReplicatedStorage").Package.Events.TP
    Event:InvokeServer(A_1)
wait(2)
elseif game.PlaceId == 5151400895 then
break
end
end
end)

spawn(function()
    wait(10)
while true do
            local Remote = game.ReplicatedStorage.Package.Events['block']

            local Arguments = {
                [1] = true
            }

            Remote:InvokeServer(unpack(Arguments))
end
end)


local B = Y.Toggle({
    Text = 'autofarm',
    Callback = function(bool)
        Settings.Variables.Farm = true
        while true and task.wait() do 
            if (not plr.PlayerGui.Main.MainFrame.Frames.Quest.Nop.Visible and true) then
                for i,v in next, workspace.Living:GetChildren() do 
                    if (v.Name == returnQuest(true) or v.Name == game:GetService("ReplicatedStorage").Package.Quests[returnQuest(true)].Objective.Value) and not plr.PlayerGui.Main.MainFrame.Frames.Quest.Nop.Visible and v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') and v.Humanoid.Health > 0 then 
                        repeat task.wait()
                            pcall(function() plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,2) end)
                            spawn(function()
                                task.wait()
                                punchRemote:FireServer('Blacknwhite27',1)
                                chakraRemote:InvokeServer('Blacknwhite27')
                                chakraRemote:InvokeServer('Blacknwhite27')
                                punchRemote:FireServer('Blacknwhite27',1)
                                meleeRemote:InvokeServer('Mach Kick','Blacknwhite27')
                                meleeRemote:InvokeServer('High Power Rush','Blacknwhite27')
                                meleeRemote:InvokeServer('Meteor Crash','Blacknwhite27')
                                meleeRemote:InvokeServer('Wolf Fang Fist','Blacknwhite27')
                                meleeRemote:InvokeServer('Spirit Barrage','Blacknwhite27')
                                meleeRemote:InvokeServer('God Slicer','Blacknwhite27')
                                meleeRemote:InvokeServer('Sledgehammer','Blacknwhite27')
                                meleeRemote:InvokeServer('Flash Kick','Blacknwhite27')
                                meleeRemote:InvokeServer('Uppercut','Blacknwhite27')
                                meleeRemote:InvokeServer('Vital Strike','Blacknwhite27')
                                meleeRemote:InvokeServer('Vanish Strike','Blacknwhite27')
                                meleeRemote:InvokeServer('Super Dragon Fist','Blacknwhite27')
                                meleeRemote:InvokeServer('Spirit Breaking Cannon','Blacknwhite27')
                                hakaiRemote:InvokeServer('Soul Punisher','["MouseHit"] = CFrame.new(Vector3.new(1378.4306640625, 18.40189552307129, 1150.8336181640625), Vector3.new(-0.14783287048339844, -0.3049262762069702, -0.9408323764801025)),["FaceMouse"] = true','Blacknwhite27')
                                hakaiRemote:InvokeServer('Destruction','["MouseHit"] = CFrame.new(Vector3.new(1378.4306640625, 18.40189552307129, 1150.8336181640625), Vector3.new(-0.14783287048339844, -0.3049262762069702, -0.9408323764801025)),["FaceMouse"] = true','Blacknwhite27')
                        end)
                        until not v or not v:FindFirstChild('Humanoid') or not v:FindFirstChild('HumanoidRootPart') or v.Humanoid.Health <= 0 or not Settings.Variables.Farm or plr.PlayerGui:WaitForChild('Main').MainFrame.Frames.Quest.Nop.Visible
                    end 
                end 
            else 
                pcall(function() questRemote:InvokeServer(workspace.Others.NPCs[returnQuest(true)]) end)
            end 
        end 
    end,
    Enabled = true
})
